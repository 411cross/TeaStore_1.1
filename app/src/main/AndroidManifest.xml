<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.peek_mapdemotest.buy_test1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity_buy"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".buy_Activity" android:parentActivityName=".StoresList"></activity>
        <activity android:name=".goods_detail" ></activity>
        <activity android:name=".register" android:parentActivityName=".MainActivity_buy"></activity>
        <activity android:name=".AddressList" android:parentActivityName=".StoresList"></activity>
        <activity android:name=".AccountMessage" android:parentActivityName=".StoresList"></activity>
        <activity android:name=".StoresList"></activity>
        <activity android:name=".ModifyMessage" android:parentActivityName=".AccountMessage"></activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"/>
    <!--在SDCard中创建与删除文件权限  -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <!-- 往SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <!--  从SDCard读取数据权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
</manifest>
